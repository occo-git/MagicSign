    public static Integer getScoreBySignInfo(SignStrength strength) {
        int score = 0;
        switch (strength) {
            case SIMPLE:  score = CurrentSettings.signScore_SIMPLE; break;
            case NORMAL:  score = CurrentSettings.signScore_NORMAL; break;
            case HARD:    score = CurrentSettings.signScore_HARD; break;
            case INSANE:  score = CurrentSettings.signScore_INSANE; break;
            case CRAZY:   score = CurrentSettings.signScore_CRAZY; break;
            default:      score = 0; break;
        }
        return score * CurrentSettings.scoreMultiplier;
    }

    public static int getSignWavesBySignInfo(SignStrength strength) {
        switch (strength) {
            case SIMPLE:  return 0;
            case NORMAL:  return 2;
            case HARD:    return 3;
            case INSANE:  return 4;
            case CRAZY:   return 5;
            default:      return 0;
        }
    }

    public static boolean isGroupAchievementType(AchievementType type) {
        return Utils.inArray(type, new AchievementType[] {
                AchievementType.SIMPLE_01_GROUP,
                AchievementType.NORMAL_02_GROUP,
                AchievementType.HARD_03_GROUP,
                AchievementType.INSANE_04_GROUP,
                AchievementType.CRAZY_05_GROUP});
    }

