    // бонус за угадывание collapsed Sign
    public static Integer getBonusBySignInfo(SignStrength strength) { return getScoreBySignInfo(strength) * CurrentSettings.bonusGuessCoeff; }

    public static SignInfo getFitSignInfo(RaySign raySign) {
        if (sInfoItems != null && raySign != null) {
            ArrayList<SignInfo> fitInfos = new ArrayList<>();
            for (SignInfo info : sInfoItems)
                if (info.check(raySign))
                    fitInfos.add(info);
            if (fitInfos.size() > 0)
                return fitInfos.get(0);
        }
        return null;
    }

    public static void recycle() {
        if (sInfos != null)
            sInfos.recycle();
        if (sInfoGroups != null)
            sInfoGroups.recycle();
        sInfos = null;
        sInfoGroups = null;
    }
