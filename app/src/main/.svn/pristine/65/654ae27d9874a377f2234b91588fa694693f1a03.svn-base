    //region add
    private static SignInfo addSignInfo(int id, float[] points, SignStrength strength) {
        SignInfo sInfo = new SignInfo(id, points, strength);
        sInfos.add(sInfo);
        return sInfo;
    }

    private static void addSignInfoGroup(int id, SignStrength strength, SignInfo info1, SignInfo info2, SignInfo info3, SignInfo info4) {
        sInfoGroups.add(new SignInfoGroup(id, strength, info1, info2, info3, info4));
    }
    //endregion

    //region get SingInfo
    private static int getInt(SignStrength strength) {
        int index = strengthInts.indexOfValue(strength);
        if (index >= 0)
            return strengthInts.keyAt(index);
        else
            return -1;
    }

    public static SignInfo getRandomInfoByParams(SignStrength strength) {
        SignInfo[] infos = getInfosByParams(strength);
        if (infos != null && infos.length > 0)
            return infos[Utils.getRandom(infos.length)];
        return null;
    }

    private static SignInfo[] getInfosByParams(SignStrength strength) {
        int s = getInt(strength);
        if (s >= 0)
            if (sInfoItemsByStrengthInt.indexOfKey(s) >= 0)
                return sInfoItemsByStrengthInt.get(s);
        return null;
    }
    //endregion

    //region get SignInfoGroup
    public static SignInfoGroup getRandomInfoGroupByParams(SignStrength strength) {
        try {

            SignInfoGroup[] infoGroups = getInfoGroupsByParams(strength);
            if (infoGroups != null && infoGroups.length > 0) {
                SignInfoGroup g = infoGroups[Utils.getRandom(infoGroups.length)];
                if (g != null)
                    return g.getClone();
            }

        } catch (Throwable t) {
            Utils.CrashReport("SignInfos.getRandomInfoGroupByParams", t);
        }
        return null;
    }

    private static SignInfoGroup[] getInfoGroupsByParams(SignStrength strength) {
        try {

            int s = getInt(strength);
            if (s >= 0)
                if (sInfoGroupItemsByStrengthInt.indexOfKey(s) >= 0)
                    return sInfoGroupItemsByStrengthInt.get(s);
        } catch (Throwable t) {
            Utils.CrashReport("SignInfos.getInfoGroupsByParams", t);
        }
        return null;
    }
    //endregion

    //region sign color/paint
    public static int getColorByAchievementType(AchievementType type) {
        switch (type) {
            case EMPTY_00:          return Color.argb(255,   0,   0,   0); // black
            case SIMPLE_01:         return Color.argb(255, 240, 240,   0); // yellow
            case NORMAL_02:         return Color.argb(255, 255, 128,   0); // orange
            case HARD_03:           return Color.argb(255, 220,   0,   0); // red
            case INSANE_04:         return Color.argb(255, 180,   0, 180); // violet
            case CRAZY_05:          return Color.argb(255,  80,  80, 180); // sea
            case SIMPLE_01_GROUP:   return Color.argb(255, 240, 240,  50); // yellow
            case NORMAL_02_GROUP:   return Color.argb(255, 255, 128,  50); // orange
            case HARD_03_GROUP:     return Color.argb(255, 220,  50,  50); // red
            case INSANE_04_GROUP:   return Color.argb(255, 180,  50, 180); // violet
            case CRAZY_05_GROUP:    return Color.argb(255, 120, 120, 180); // sea
            case FINAL_10:          return Color.argb(255,  50,  50,  50); // black
            default:                return Color.argb(255, 255, 255, 255); // white
        }
    }

    private static Paint getPaintByAchievementType(AchievementType type) {
        return PaintUtils.getPaint(getColorByAchievementType(type));
    }

    public static Paint getPaintBySignInfo(SignStrength strength) {
        switch (strength) {
            case SIMPLE:  return getPaintByAchievementType(AchievementType.SIMPLE_01); // yellow
            case NORMAL:  return getPaintByAchievementType(AchievementType.NORMAL_02); // orange
            case HARD:    return getPaintByAchievementType(AchievementType.HARD_03);   // red
            case INSANE:  return getPaintByAchievementType(AchievementType.INSANE_04); // violet
            case CRAZY:   return getPaintByAchievementType(AchievementType.CRAZY_05);  // sea
            default:      return getPaintByAchievementType(AchievementType.EMPTY_00);  // black
        }
    }

    public static Paint getPaintBySignInfoGroup(SignStrength strength) {
        switch (strength) {
            case SIMPLE:  return getPaintByAchievementType(AchievementType.SIMPLE_01_GROUP);    // yellow
            case NORMAL:  return getPaintByAchievementType(AchievementType.NORMAL_02_GROUP);    // orange
            case HARD:    return getPaintByAchievementType(AchievementType.HARD_03_GROUP);      // red
            case INSANE:  return getPaintByAchievementType(AchievementType.INSANE_04_GROUP);    // violet
            case CRAZY:   return getPaintByAchievementType(AchievementType.CRAZY_05_GROUP);     // sea
            default:      return getPaintByAchievementType(AchievementType.EMPTY_00);           // black
        }
    }
